<Application x:Class="Form.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Form"
             xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <Application.Resources>
        <system:Double x:Key="MainWindowHeight">350</system:Double>
        <system:Double x:Key="MainWindowWidth">600</system:Double>

        <system:Double x:Key="MenuWidth">100</system:Double>


        <system:Double x:Key="SettingsHeight">350</system:Double>
        <system:Double x:Key="SettingsWidth">500</system:Double>
    </Application.Resources>
</Application>
